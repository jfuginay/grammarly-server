# Application
NODE_ENV=development
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/grammarly?schema=public

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=development_secret_key_change_in_production
JWT_EXPIRES_IN=24h

# External APIs
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=http://localhost:3000

# Logging
LOG_LEVEL=debug